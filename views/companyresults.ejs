<h1 class="title" style="text-align: center;" >CURRENT COMPANIES DRIVES RESULT</h1>
<% ResultsList.forEach(result => { %>
<div class="tile is-ancestor mt-6" style="width: 30%; display: block; margin: 0 auto;">
    <div class="tile is-parent">
        <div class="tile is-child " id="events">
            <div class="tile is-ancestor">
                <div class="tile is-parent">
                    <article class="tile is-child ">
                        <div class="card mx-4" >
                        
                            <div class="card-content">
                                <p class="title"><%= result.companyname %></p>
                                <div class="content">
                                    <p>Salary: <strong><%= result.salary %></strong> </p>
                                    <p>Role: <strong><%= result.role %></strong></p>
                                    <a href="/register_job">
                                        <button class="button is-light is-outlined mt-3 is-pulled-right"
                                            style="background-color: rgba(105, 0, 255, 0.11);color: #6900FF;"><strong><a href="<%= result.link %>" style="text-decoration: none; color: #6900FF;">Download
                                                Now</a></strong></button>
                                    </a>
                                    <br>
                                    <time datetime="2016-1-1"><%= result.date %></time>
                                </div>
                            </div>
                        </div>
                    </article>

                </div>
            </div>

        </div>
    </div>
</div>
<%})%>
